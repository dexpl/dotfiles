# VirtualBox
showvminfo () {
	[ -z "$1" ] && return 1
	vboxmanage showvminfo "$1" --machinereadable | less -F
}

lsvms () {
	vboxmanage list vms
}

lsrunningvms () {
	vboxmanage list runningvms
}

lssnapshots () {
	[ -z "$1" ] && return 1
	vboxmanage snapshot "$1" list --machinereadable | less -F
}
