# Punctuation
<Multi_key> <Cyrillic_BE> <Cyrillic_BE>         : "«"   guillemotleft # LEFT-POINTING DOUBLE ANGLE QUOTATION MARK
<Multi_key> <Cyrillic_YU> <Cyrillic_YU>         : "»"   guillemotright # RIGHT-POINTING DOUBLE ANGLE QUOTATION MARK
<Multi_key> <period> <colon>                    : "…"   ellipsis # ELLIPSIS


# Arrows
# Simple
<Multi_key> <less> <minus>              : "←" U2190 # LEFTWARDS ARROW
<Multi_key> <bar> <asciicircum>         : "↑" U2191 # UPWARDS ARROW
<Multi_key> <minus> <greater>           : "→" U2192 # RIGHTWARDS ARROW
<Multi_key> <bar> <v>                   : "↓" U2193 # DOWNWARDS ARROW
<Multi_key> <less> <greater> <minus>    : "↔" U2194 # LEFT RIGHT ARROW
<Multi_key> <minus> <less> <greater>    : "↔" U2194 # LEFT RIGHT ARROW
<Multi_key> <bar> <v> <asciicircum>     : "↕" U2195 # UP DOWN ARROW
<Multi_key> <bar> <asciicircum> <v>     : "↕" U2195 # UP DOWN ARROW
<Multi_key> <backslash> <asciicircum>   : "↖" U2196 # NORTH WEST ARROW
<Multi_key> <slash> <asciicircum>       : "↗" U2197 # NORTH EAST ARROW
<Multi_key> <backslash> <v>             : "↘" U2198 # SOUTH EAST ARROW
<Multi_key> <slash> <v>                 : "↙" U2199 # SOUTH WEST ARROW

# Double
<Multi_key> <less> <equal>              : "⇐" U21D0 # LEFTWARDS DOUBLE ARROW
<Multi_key> <bar> <bar> <asciicircum>       : "⇑" U21D1 # UPWARDS DOUBLE ARROW
<Multi_key> <equal> <greater>   : "⇒" U21D2 # RIGHTWARDS DOUBLE ARROW
<Multi_key> <bar> <bar> <v>                     : "⇓" U21D3 # DOWNWARDS DOUBLE ARROW
<Multi_key> <less> <greater> <equal>    : "⇔" U21D4 # LEFT RIGHT DOUBLE ARROW
<Multi_key> <equal> <less> <greater>    : "⇔" U21D4 # LEFT RIGHT DOUBLE ARROW

# Arrows by arrow keys
<Multi_key> <LEFT> <LEFT>               : "←" U2190 # LEFTWARDS ARROW
<Multi_key> <LEFT> <2>                  : "⇐" U21D0 # LEFTWARDS DOUBLE ARROW
<Multi_key> <LEFT> <RGHT>               : "↔" U2194 # LEFT RIGHT ARROW

# Math
<Multi_key> <equal> <slash>     : "≠"   U2260 # NOT EQUAL TO
<Multi_key> <slash> <equal>     : "≠"   U2260 # NOT EQUAL TO
<Multi_key> <minus> <equal>     : "≡"   U2261 # IDENTICAL TO
<Multi_key> <equal> <minus>     : "≡"   U2261 # IDENTICAL TO

